<?xml version="1.0" encoding="UTF-8"?><web-app version="2.5" 	xmlns="http://java.sun.com/xml/ns/javaee" 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">  <display-name></display-name>  <servlet>    <servlet-name>PhotoUploadServlet</servlet-name>    <servlet-class>com.upload.servlet.PhotoUploadServlet</servlet-class>    <init-param>    	<param-name>savePath</param-name>    	<param-value>uploads</param-value>    </init-param>      </servlet>  <servlet-mapping>    <servlet-name>PhotoUploadServlet</servlet-name>    <url-pattern>/servlet/PhotoUploadServlet</url-pattern>  </servlet-mapping>    <filter>  	<filter-name>enCoding</filter-name>  	<filter-class>com.upload.filter.EnCodingFilter</filter-class>  </filter>	  <filter-mapping>  	<filter-name>enCoding</filter-name>  	<url-pattern>/*</url-pattern>  </filter-mapping>  <welcome-file-list>    <welcome-file>index.jsp</welcome-file>  </welcome-file-list></web-app>